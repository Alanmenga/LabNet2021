@{
    ViewBag.Title = "Index";
}

<!DOCTYPE html>

<h2>Lista de Usuarios</h2>
<table class="table">
    <thead>
        <tr>
            <td scope="col">ID</td>
            <td scope="col">Nombre</td>
            <td scope="col">Usuario</td>
            <td scope="col">Email</td>
            <td scope="col">Numero</td>
            <td scope="col">Sitio Web</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var usuario in Model)
        {
            <tr>
                <td>@usuario.id </td>
                <td>@usuario.name</td>
                <td>@usuario.username</td>
                <td>@usuario.email</td>
                <td>@usuario.phone</td>
                <td>@usuario.website</td>
            </tr>
        }
    </tbody>
</table>

